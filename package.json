{
  "name": "discordsoundboard",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ggresillion/discordsoundboard",
  "license": "MIT",
  "scripts": {
    "start": "run-p start:backend start:frontend",
    "start:backend": "cd backend && $(go env GOPATH)/bin/air",
    "start:frontend": "cd frontend && $npm_execpath run dev",
    "postinstall": "run-s postinstall:backend postinstall:frontend",
    "postinstall:backend": "cd backend && go mod download",
    "postinstall:frontend": "cd frontend && $npm_execpath install"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
