{
  "name": "discord-sound-board",
  "version": "0.0.0",
  "description": "A web client discord sound board for Discord !",
  "author": "Guillaume Gr√©sillion",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=development nodemon",
    "start:debug": "NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@discordjs/opus": "^0.6.0",
    "@discordjs/voice": "^0.7.2",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.2.0",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.2.0",
    "@nestjs/platform-express": "^8.2.0",
    "@nestjs/platform-socket.io": "^8.2.0",
    "@nestjs/testing": "^8.2.0",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.2.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "discord.js": "^13.3.1",
    "fluent-ffmpeg": "^2.1.2",
    "get-audio-duration": "^3.0.0",
    "passport": "^0.5.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.2",
    "prism-media": "^1.2.9",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "scrape-yt": "^1.4.6",
    "socket.io": "^4.3.2",
    "sodium": "^3.0.2",
    "typeorm": "^0.2.29",
    "typescript": "^4.0.5",
    "uuid": "^8.3.1",
    "ytdl-core": "^4.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.8",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/node-fetch": "^3.0.3",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/youtube-dl": "^3.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "6.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
